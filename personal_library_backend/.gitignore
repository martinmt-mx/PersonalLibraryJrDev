# Temporary files generated by your text editor or operating system
# belong in git's global ignore instead:
# `$XDG_CONFIG_HOME/git/ignore` or `~/.config/git/ignore`

# Ignore bundler config.
.bundle

# Ignore all environment files.
.env
.env.development.local
.env.test.local
.env.production.local

# Ignore sensitive configuration files
/config/master.key
/config/credentials.yml.enc
/config/database.yml

# Provide an example configuration for database
/config/database.example.yml

# Logs and temp files
/log/*
/tmp/*
!/log/.keep
!/tmp/.keep

# Ignore pidfiles, but keep the directory.
tmp/pids/*
!/tmp/pids/
!/tmp/pids/.keep

# Ignore storage (uploaded files in development and any SQLite databases).
/storage/*
!/storage/.keep
/tmp/storage/*
!/tmp/storage/
!/tmp/storage/.keep

# Ignore public assets (precompiled)
public/assets
public/packs
public/packs-test
public/system
public/uploads

# Ignore Yarn & Node modules
node_modules/
yarn-error.log
.yarn-integrity

# Ignore cache and compiled files
.byebug_history
**/.bundle
/vendor/bundle

# Ignore editor & OS specific files
*.swp
*.swo
*~
.project
.idea
.vscode/
.DS_Store
*.sublime*

# Ignore test results
/spec/examples.txt
/spec/tmp

# Coverage reports
/coverage/
